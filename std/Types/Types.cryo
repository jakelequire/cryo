namespace Types;

struct String {
    value: string;

    constructor(val: string) {
        this.value = val;
    }
}

struct Int {
    value: int;

    constructor(val: int) {
        this.value = val;
    }

    toString() -> String {
        const str: string = intToString(this.value);
        const strObj: String = new String(str);
        return strObj;
    }
}

struct Boolean {
    value: boolean;

    constructor(val: boolean) {
        this.value = val;
    }
}

