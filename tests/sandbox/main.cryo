namespace Main;

extern function printInt(i: int) -> void;
extern function printStr(s: string) -> void;
extern function intToString(num: int) -> string;

struct Int {
    value: int;

    constructor(val: int) {
        this.value = val;
    }

    toString() -> string {
        return intToString(this.value);
    }
}

struct Foo {
    bar: int;
    baz: string;
    buz: Int;
}

public function anotherTest() -> void {
    const fooTest: int = 8008;
    return;
}

public function testFunc() -> void {
    const intTest: Int = 420;
    const intTestTwo: int = intTest.value;
    printInt(intTestTwo);
    printInt(intTest.value);

    const strVal: string = intTest.toString();
    printStr(strVal);
    printStr(intTest.toString());
    return;
}

public function main() -> void {
    const fooInt: int = 34;
    printInt(fooInt);

    const fooStr: string = "This is pain";
    printStr(fooStr);
    printStr("Oof!");

    testFunc();

    return;
}
