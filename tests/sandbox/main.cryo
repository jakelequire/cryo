namespace Main;

extern function printInt(i: int) -> void;
extern function printStr(s: string) -> void;
extern function intToString(num: int) -> string;

struct Int {
    // The default keyword is used to specify the default primary property
    // value of the struct field. If `default` is not provided,
    // The constructor will need to be explicitly called with
    // the field value.
    value: int;

    // Constructor for the Int struct
    constructor(val: int) {
        this.value = val;
    }

    // Method for the Int struct
    toString() -> string {
        return intToString(this.value);
    }
}

public function testFunc() -> void {
    const intTest: Int = 420;
    printInt(intTest.value);
    return;
}

public function main() -> void {
    const fooInt: int = 69;
    const barInt: int = 420;
    printInt(fooInt);

    const fooStr: string = "I'm in pain";
    printStr(fooStr);
    printStr("Onichan!");

    testFunc();

    return;
}
