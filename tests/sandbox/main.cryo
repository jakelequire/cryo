namespace Main;

extern function printInt(i: int) -> void;

struct Int {
    // The default keyword is used to specify the default
    // value of the struct field. If `default` is not provided,
    // The constructor will need to be explicitly called with
    // the field value.
    default value: int;

    // Constructor for the Int struct
    constructor(value: int) {
        this.value = value;
    }

    // Methods for the Int struct
    toString() -> string {
        const i: int = this.value;
        return intToString(i);
    }
}

public function main() -> void {
    printInt(42);
    return;
}
