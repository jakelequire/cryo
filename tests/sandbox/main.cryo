namespace Main;

extern function printInt(i: int) -> void;
extern function printStr(s: string) -> void;
extern function intToString(num: int) -> string;

struct Int {
    value: int;

    constructor(val: int) {
        this.value = val;
    }

    toString() -> string {
        return intToString(this.value);
    }
}

struct Foo {
    bar: int;
    baz: string;
    buz: Int;
}

function binOpTest() -> void {
    const addTest: int = 34 + 36;
    printStr("Addition test: ");
    printInt(addTest);
    
    const subTest: int = 34 - 36;
    printStr("Subtraction test: ");
    printInt(subTest);

    const mulTest: int = 16 * 16;
    printStr("Multiplication test: ");
    printInt(mulTest);

    const divTest: int = 10 / 2;
    printStr("Division test: ");
    printInt(divTest);

    const modTest: int = 10 % 3;
    printStr("Modulus test: ");
    printInt(modTest);

    return;
}

function structTesting() -> void {
    const intTest: Int = 1234;
    const intTestTwo: int = intTest.value;
    printInt(intTestTwo);
    printInt(intTest.value);

    const strVal: string = intTest.toString();
    printStr(strVal);
    printStr(intTest.toString());
    return;
}

function main() -> void {
    const fooInt: int = 34;
    printInt(fooInt);

    const fooStr: string = "This is pain";
    printStr(fooStr);
    printStr("Oof!");

    structTesting();
    binOpTest();

    return;
}
