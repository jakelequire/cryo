namespace Testing;


class TestingClass {
    public value: int;

    constructor() {}

    public static methodOne() -> void {
        const testInt: int = 42;
        printInt(testInt);
        return;
    }

    public static methodTwo() -> void {
        const testIntTwo: int = 69;
        printInt(testIntTwo);
        return;
    }

    public static methodThree() -> void {
        return;
    }
}

function testUsingKw() -> void {
    const testIntThree: Int = 18;
    printInt(testIntThree.value);

    return;
}

function testUsingKwTwo() -> void {
    const testStr: String = "Hello, World!";
    printStr(testStr.value);
    
    return;
}

function main(argc: int, argv: string[]) -> int {
    printStr("---------- Main Function ----------");

    TestingClass::methodOne();
    TestingClass::methodTwo();
    TestingClass::methodThree();

    testUsingKw();

    printStr("Testing Complete");

    return 0;
}
