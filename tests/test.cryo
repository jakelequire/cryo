namespace Main;

extern function printStr(str: string) -> void;
extern function printInt(num: int) -> void;
extern function sys_exit(code: int) -> void;

public function fibbonacci(n: int) -> void {
    mut a: int = 0;
    mut b: int = 1;
    mut c: int = 0;
    mut num: int = n;

    while (num > 0) {
        c = a + b;
        a = b;
        b = c;
        num = num - 1;
    }

    printInt(c);
    return;
}


public function main() -> void {
    printStr("Hello, World!");

    mut intValue: int = 69;
    printInt(intValue);

    intValue = 420;
    printInt(intValue);

    return;
}
