namespace Main;

extern function printStr(str: string) -> void;
extern function printInt(num: int) -> void;
extern function sys_exit(code: int) -> void;
extern function type_of(typeOf: Any) -> void;

public function fibonacci(n: int) -> void {
    mut a: int = 0;
    mut b: int = 1;
    mut c: int = 0;

    while (n > 0) {
        c = a + b;
        a = b;
        b = c;

        printInt(a);
        n = n - 1;
    }

    return;
}


public function main() -> void {
    printStr("Fibonacci sequence up to 16:");
    fibonacci(16);

    printStr("This is a test\n Another Test\t");

    const testString: string = "This is a test";
    const testInt: int = 69;

    type_of("Hello");
    type_of(34);
    type_of(true);

    return;
}

