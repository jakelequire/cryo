public function fibonacci(n: int) -> void {
    mut a: int = 0;
    mut b: int = 1;
    mut c: int = 0;
    const aValStr: string = "A Value: ";
    const bValStr: string = "B Value: ";
    const cValStr: string = "C Value: ";

    printStr("Fibbonacci Sequence: ");
    printStr("Variables State before loop: ");
    printStr(aValStr);
    printInt(a);
    printStr(bValStr);
    printInt(b);
    printStr(cValStr);
    printInt(c);

    printStr(" ");
    printStr(" ");
    printStr("Going into loop");
    while (n > 0) {
        c = a + b;
        a = b;
        b = c;

        printInt(a);
        n = n - 1;
    }
    return;
}


